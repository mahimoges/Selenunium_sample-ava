import java.util.Scanner;

public class sub {
	
	
	public static void main(String[]args) {
		
	 Scanner scan = new Scanner(System.in);
	 int sub1= scan.nextInt(10);
	 int sub2 = scan.nextInt(10);
	 int sub = sub1 + sub2;
	 System.out.println("substraction of these cumbers;" + sub);
	 
	
	 
			 
	}

}


