 class baby {
	int age;
	String name;
	boolean sex;
	int ssn;
	
	public static void main(String[] args, String amanu) {
		baby boybaby = new baby();
		boybaby.age = 1;
		boybaby.sex = true;
		boybaby.name = amanu;
		boybaby.ssn = 123456;
		System.out.println(boybaby.name);
	}
}